<%= form_with(model: quiz, local: true) do |form| %>
  <% if quiz.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(quiz.errors.count, "error") %></h2>
      <ul class="alert alert-warning">
      <% quiz.errors.full_messages.each do |message| %>
        <li class="list-unstyled font-weight-bold"><%= message %></li>
      <% end %>
      </ul>
    </div>
<% end %>

    <div class="alert alert-dark my-5"><i class="fa fa-info-circle" aria-hidden="true"></i>基本データ<i class="fa fa-info-circle" aria-hidden="true"></i></div>

    <div class="form-group">
        <%= form.label :author, "作者", class:"control-label" %>
        <div class="font-weight-bolder"><%= @nickname %></div>
    </div>

    <div class="form-group">
        <%= form.label :title, "タイトル", class:"control-label" %>
        <%= form.text_field :title, class:"form-control d-inline-block", placeholder:"診断タイトル" %>
    </div>

    <div class="form-group">
        必須項目です。２００文字まで入力できます。
    </div>

    <div class="form-group">
        <%= form.label :title_img, "タイトル用動画か画像", class:"control-label" %>
        <%= form.text_field :title_img, class:"form-control d-inline-block", placeholder:"ここに画像や動画のリンクを貼ると、タイトルに画像や動画を付けられます" %>
    </div>

    <div class="form-group">
        <%= form.label :title_img_option, "タイトル用動画か画像のオプション", class:"control-label " %>
        <%= form.select :title_img_option, [['動画も画像も無用', 0], ['画像もしくはGIFをつける', 1], ['動画をつける', 2]], class:"form-control d-inline-block" %>
